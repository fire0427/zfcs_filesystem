# @name GPIO set test including set gpio to 'in', 'out' etc
# @desc Set gpio pin in each bank to 'in' or 'out' through sysfs. Before run this test, kernel may be required to be modified to add entries for pin to test
# @requires gpio && am180x-evm 

GPIO_M_FUNC_DIR_IN_AM180X-EVM_BANK0 source "common.sh"; gpio.sh -b "0" -g "0" -l "2" -t "in"
GPIO_M_FUNC_DIR_IN_AM180X-EVM_BANK1 source "common.sh"; gpio.sh -b "1" -g "15" -l "2" -t "in"
GPIO_M_FUNC_DIR_IN_AM180X-EVM_BANK2 source "common.sh"; gpio.sh -b "2" -g "10" -l "2" -t "in"
GPIO_M_FUNC_DIR_IN_AM180X-EVM_BANK3 source "common.sh"; gpio.sh -b "3" -g "8" -l "2" -t "in"
GPIO_M_FUNC_DIR_IN_AM180X-EVM_BANK4 source "common.sh"; gpio.sh -b "4" -g "8" -l "2" -t "in"
GPIO_M_FUNC_DIR_IN_AM180X-EVM_BANK5 source "common.sh"; gpio.sh -b "5" -g "8" -l "2" -t "in"
GPIO_M_FUNC_DIR_IN_AM180X-EVM_BANK6 source "common.sh"; gpio.sh -b "6" -g "8" -l "2" -t "in"
GPIO_M_FUNC_DIR_IN_AM180X-EVM_BANK7 source "common.sh"; gpio.sh -b "7" -g "4" -l "2" -t "in"
GPIO_M_FUNC_DIR_IN_AM180X-EVM_BANK8 source "common.sh"; gpio.sh -b "8" -g "12" -l "2" -t "in"


GPIO_M_FUNC_DIR_OUT_AM180X-EVM_BANK0 source "common.sh"; gpio.sh -b "0" -g "0" -l "2" -t "out"
GPIO_M_FUNC_DIR_OUT_AM180X-EVM_BANK1 source "common.sh"; gpio.sh -b "1" -g "15" -l "2" -t "out"
GPIO_M_FUNC_DIR_OUT_AM180X-EVM_BANK2 source "common.sh"; gpio.sh -b "2" -g "10" -l "2" -t "out"
GPIO_M_FUNC_DIR_OUT_AM180X-EVM_BANK3 source "common.sh"; gpio.sh -b "3" -g "8" -l "2" -t "out"
GPIO_M_FUNC_DIR_OUT_AM180X-EVM_BANK4 source "common.sh"; gpio.sh -b "4" -g "8" -l "2" -t "out"
GPIO_M_FUNC_DIR_OUT_AM180X-EVM_BANK5 source "common.sh"; gpio.sh -b "5" -g "8" -l "2" -t "out"
GPIO_M_FUNC_DIR_OUT_AM180X-EVM_BANK6 source "common.sh"; gpio.sh -b "6" -g "8" -l "2" -t "out"
GPIO_M_FUNC_DIR_OUT_AM180X-EVM_BANK7 source "common.sh"; gpio.sh -b "7" -g "4" -l "2" -t "out"
GPIO_M_FUNC_DIR_OUT_AM180X-EVM_BANK8 source "common.sh"; gpio.sh -b "8" -g "12" -l "2" -t "out"


GPIO_M_FUNC_EDGE_AM180X-EVM_BANK0 source "common.sh"; gpio.sh -b "0" -g "0" -l "2" -t "edge"
GPIO_M_FUNC_EDGE_AM180X-EVM_BANK1 source "common.sh"; gpio.sh -b "1" -g "15" -l "2" -t "edge"
GPIO_M_FUNC_EDGE_AM180X-EVM_BANK2 source "common.sh"; gpio.sh -b "2" -g "10" -l "2" -t "edge"
